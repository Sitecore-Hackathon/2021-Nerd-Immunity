<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <events>
        <event name="item:saving">
            <handler type="NerdImmunity2021.Feature.Cloudflare.Pipelines.CloudflareItemSaving, NerdImmunity2021.Feature.Cloudflare" method="OnItemSaving" />
        </event>
        <event name="publish:itemProcessed">
            <handler type="NerdImmunity2021.Feature.Cloudflare.Pipelines.CloudflareItemPublishProcessed, NerdImmunity2021.Feature.Cloudflare" method="ItemProcessed" />
        </event>
    </events>
  </sitecore>
</configuration>
