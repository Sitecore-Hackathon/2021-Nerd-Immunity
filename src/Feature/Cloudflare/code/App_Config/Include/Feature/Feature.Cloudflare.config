<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
      <pipelines>
          <publish>
              <processor patch:after="*[@type='Sitecore.Publishing.Pipelines.Publish.ProcessQueue, Sitecore.Kernel']" type="NerdImmunity2021.Feature.Cloudflare.Pipelines.CloudflareItemPublishProcessor, NerdImmunity2021.Feature.Cloudflare"/>
          </publish>
      </pipelines>
      <events>
        <event name="item:saving">
            <handler type="NerdImmunity2021.Feature.Cloudflare.Pipelines.CloudflareItemSaving, NerdImmunity2021.Feature.Cloudflare" method="OnItemSaving" />
        </event>
      </events>
  </sitecore>
</configuration>
